{
  "name": "@cloaklink/api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --loader ts-node/esm src/server.ts",
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node-dev --respawn --transpile-only --esm src/server.ts",
    "prisma": "prisma",
    "migrate": "prisma migrate deploy --schema prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --create-only --schema prisma/schema.prisma",
    "db:reset": "prisma migrate reset --force --schema prisma/schema.prisma",
    "generate": "prisma generate",
    "db:seed": "prisma db seed --schema prisma/schema.prisma",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "pretest": "npm run generate",
    "test": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "prisma": {
    "seed": "ts-node --esm prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@solana/web3.js": "^1.95.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "morgan": "^1.10.1",
    "nanoid": "^5.1.6",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "prisma": "5.22.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
